#include "../header-vmdl/externc.vmdh"


(needContext, makeInline) string_to_object : String -> JSValue
string_to_object(v){
    top: match(v){
        case(String v){
            return new_normal_string_object(v);
        }
        case(true){
            type_error("string expected in string_to_object");
            return JS_UNDEFINED;
        }
    }
}