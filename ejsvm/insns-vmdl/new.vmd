#include "../header-vmdl/externc.vmdh"

(vmInstruction, needContext, triggerGC, tailCall)
new : JSValue -> JSValue

new (con) {
  top: match (con) {
    case (Function con) {
      Save_context();
      JSObject o = Create_simple_object_with_constructor(con);
      Update_context();
      return o;
    }
    case (true) {
      return JS_UNDEFINED;
    }
  }
}