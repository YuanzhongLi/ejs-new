#include "../header-vmdl/externc.vmdh"

(vmInstruction, needContext, triggerGC, tailCall)
new : JSValue -> JSValue

new (con) {
  top: match (con) {
    case (Function con) {
      save_context();
      JSValue o = new_normal_object();
      update_context();
      return initialize_new_object(con, o);
    }
    case (true) {
      return JS_UNDEFINED;
    }
  }
}