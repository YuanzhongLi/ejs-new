(vmInstruction, needContext, triggerGC, tailCall)
new : JSValue -> JSObject

new (con) {
  top: match (con) {
    case (Function con) {
      Save_context();
      JSValue o = New_normal_object();
      Update_context();
      return Initialize_new_object(con, o);
    }
    case (true) {
      return JS_undefined();
    }
  }
}