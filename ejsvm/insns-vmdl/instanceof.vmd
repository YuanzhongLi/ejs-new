#include "patterns.vmdh"

#include "externc.vmdh"

(vmInstruction, tailCall)
instanceof : (JSValue, JSValue) -> JSValue

instanceof (v1, v2) {
    top: match (v1, v2) {
      case (object v1 && object v2) {
          return Instanceof(v1, v2);
      }
      case (true) {
          return JS_FALSE;
      }
    }
}
